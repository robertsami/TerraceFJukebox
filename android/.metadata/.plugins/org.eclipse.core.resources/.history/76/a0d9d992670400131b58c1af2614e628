package tfc.princeton.terracefjukebox.util;

public class SongInfo {
	/**
	 * The name of the song
	 */
	public String song;
	/**
	 * The song's artist
	 */
	public String artist;
	/**
	 * The album title of this song
	 */
	public String album;
	/**
	 * The play time of this song in seconds
	 */
	public int playTimeSeconds;
}
